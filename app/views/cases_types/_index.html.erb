



    <h2>类型一览</h2>


<table id="rounded-corner" summary="">
    <thead>
      <tr>
            <th scope="col" class="rounded">类型名称</th>
            <th scope="col" class="rounded">类型描述</th>
            <th scope="col" class="rounded">编辑</th>
            <th scope="col" class="rounded-q4">删除</th>
        </tr>
    </thead>
    <tbody>
      <% if @cases_types.blank? %>
        <tr><td colspan="4">没有类型。</td></tr>

      <% else %>
      <% @cases_types.each do |c|%>
        <tr>
          <td><%= c.name%></td>
          <td><%= c.description%></td>
          <td><%= link_to image_tag("user_edit.png"), edit_cases_type_path(c)%></td>
          <td>
            <%= link_to image_tag("trash.png"), cases_type_path(c), data: { confirm: '确认删除该类型吗?' }, :method => "delete" %>
            </td>
        </tr>
        <% end %>
      <% end%>



    </tbody>
</table>

